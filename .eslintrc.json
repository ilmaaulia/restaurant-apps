{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "always"
            }
        ],
        "no-console": "off",
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "arrow-parens": 0,
        "no-restricted-globals": 0
    }
}
